# LLM Provider API Keys
DEEPINFRA_API_KEY=your_deepinfra_key_here
OPENAI_API_KEY=your_openai_key_here

# Ollama Configuration (for local models)
OLLAMA_BASE_URL=http://localhost:11434

# Default LLM Settings
DEFAULT_PROVIDER=deepinfra
DEFAULT_MODEL=meta-llama/Llama-3.3-70B-Instruct-Turbo

# Web Scraping Configuration
WEB_SCRAPING_ENABLED=true
WEB_SCRAPING_TIMEOUT=30
WEB_SCRAPING_MAX_CONCURRENT=5
WEB_SCRAPING_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36

# Search Configuration
DEFAULT_SEARCH_REGION=ua-uk
DEFAULT_SEARCH_RESULTS=5
MAX_SEARCH_RESULTS=15
